    "#选择排序\n",
    "def choose_sort(a):\n",
    "    for i in range(len(a)-1):\n",
    "        min_index=i+1\n",
    "        for j in range(i+1,len(a)):\n",
    "            if a[j]<a[min_index]:\n",
    "                min_index=j\n",
    "        a[i],a[min_index]=a[min_index],a[i]\n",
    "    return a\n",
    "a=[9,8,7,6,5,4,3,2,1]\n",
    "print(choose_sort(a))"
   ]
